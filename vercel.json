{
    "build": {
        "env": {
            "GO_BUILD_FLAGS": "-ldflags '-s -w'"
        }
    },
    "functions": {
        "api/*.go": {
            "runtime": "go1.x"
        }
    },
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/price.go"
        }
    ]
}
